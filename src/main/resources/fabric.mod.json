{
  "schemaVersion": 1,
  "id": "examplemod",
  "version": "${version}",

  "name": "Example Mod",
  "description": "This mod aims to help new BTA modders.",
  "authors": [
    "Turnip Labs"
  ],
  "contact": {
    "homepage": "",
    "sources": ""
  },

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "scala",
        "value": "turniplabs.examplemod.ExampleMod"
      }
		],
		"beforeGameStart": [
			{
				"adapter": "scala",
				"value": "turniplabs.examplemod.ExampleMod"
			}
		],
		"afterGameStart": [
			{
				"adapter": "scala",
				"value": "turniplabs.examplemod.ExampleMod"
			}
		],
		"recipesReady": [
			{
				"adapter": "scala",
				"value": "turniplabs.examplemod.ExampleMod"
			}
		]
  },
  "mixins": [
    "examplemod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.7",
    "kotori_scala": ">=2.1.0"
  },
  "suggests": {
  }
}
